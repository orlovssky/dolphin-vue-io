{
  "name": "dolphin-next",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "build": "vite build"
  },
  "dependencies": {
    "@mdi/font": "6.5.95",
    "axios": "^0.26.1",
    "io-library": "^0.0.23",
    "vee-validate": "^4.5.10",
    "vue": "^3.2.31",
    "vue-i18n": "^9.2.0-beta.32",
    "vue-router": "^4.0.12",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "@vitejs/plugin-vue": "^2.2.0",
    "@vue/cli-plugin-babel": "5.0.1",
    "@vue/cli-plugin-eslint": "~5.0.1",
    "@vue/cli-plugin-router": "~5.0.1",
    "@vue/cli-plugin-typescript": "~5.0.1",
    "@vue/cli-plugin-vuex": "~5.0.1",
    "@vue/cli-service": "5.0.1",
    "@vue/eslint-config-typescript": "^10.0.0",
    "eslint": "^8.9.0",
    "eslint-plugin-arca": "^0.13.3",
    "eslint-plugin-vue": "^8.4.1",
    "sass": "^1.49.7",
    "sass-loader": "^12.6.0",
    "typescript": "^4.5.5",
    "vite": "^2.8.3",
    "vue-eslint-parser": "^8.2.0",
    "vue-tsc": "^0.29.8"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:vue/vue3-recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "parser": "vue-eslint-parser",
    "parserOptions": {
      "ecmaVersion": "latest",
      "parser": "@typescript-eslint/parser",
      "sourceType": "module"
    },
    "plugins": [
      "vue",
      "@typescript-eslint",
      "arca"
    ],
    "rules": {
      "no-multiple-empty-lines": [
        "error",
        {
          "max": 1,
          "maxEOF": 1
        }
      ],
      "quotes": [
        2,
        "single"
      ],
      "semi": [
        "error",
        "always"
      ],
      "indent": [
        2,
        2
      ],
      "arca/import-align": 2,
      "arca/import-ordering": 2,
      "arca/newline-after-import-section": 2
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
