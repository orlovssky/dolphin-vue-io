{
  "name": "dolphin-next",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "build": "vite build"
  },
  "dependencies": {
    "@mdi/font": "^6.5.95",
    "axios": "^0.26.1",
    "io-library": "^0.0.33",
    "vee-validate": "^4.5.10",
    "vue": "^3.2.31",
    "vue-i18n": "^9.1.9",
    "vue-router": "^4.0.14",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "@vitejs/plugin-vue": "^2.2.4",
    "@vue/eslint-config-typescript": "^10.0.0",
    "eslint": "^8.11.0",
    "eslint-plugin-arca": "^0.15.0",
    "eslint-plugin-vue": "^8.5.0",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0",
    "typescript": "^4.6.2",
    "vite": "^2.8.6",
    "vue-eslint-parser": "^8.3.0",
    "vue-tsc": "^0.33.2-patch.1",
    "webpack": "^5.70.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "vue/setup-compiler-macros": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:vue/vue3-recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "parser": "vue-eslint-parser",
    "parserOptions": {
      "ecmaVersion": "latest",
      "parser": "@typescript-eslint/parser",
      "sourceType": "module"
    },
    "plugins": [
      "vue",
      "@typescript-eslint",
      "arca"
    ],
    "rules": {
      "no-multiple-empty-lines": [
        "error",
        {
          "max": 1,
          "maxEOF": 1
        }
      ],
      "quotes": [
        2,
        "single"
      ],
      "semi": [
        "error",
        "always"
      ],
      "indent": [
        2,
        2
      ],
      "arca/import-align": 2,
      "arca/import-ordering": 2,
      "arca/newline-after-import-section": 2
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
